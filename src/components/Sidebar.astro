---
const { headings } = Astro.props;
---

<!-- <aside class="w-64 p-4 border-r border-gray-300 sticky top-0 h-screen overflow-y-auto"> -->
 <div>
  <nav>
    <ul class="space-y-2">
      {headings.map((h) => (
        <li >
            <div class={`ml-${(h.depth - 1) * 4}`}>
          <a href={`#${h.slug}`} class="hover:underline">
            {h.text}
          </a>
          </div>
        </li>
      ))}
    </ul>
  </nav>
  </div>
<!-- </aside> -->