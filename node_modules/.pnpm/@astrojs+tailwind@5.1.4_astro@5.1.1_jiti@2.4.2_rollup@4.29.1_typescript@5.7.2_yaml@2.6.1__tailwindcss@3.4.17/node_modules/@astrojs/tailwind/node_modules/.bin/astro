#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ben/Documents/project/website/cmhben/node_modules/.pnpm/astro@5.1.1_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/node_modules:/home/ben/Documents/project/website/cmhben/node_modules/.pnpm/astro@5.1.1_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules:/home/ben/Documents/project/website/cmhben/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ben/Documents/project/website/cmhben/node_modules/.pnpm/astro@5.1.1_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/node_modules:/home/ben/Documents/project/website/cmhben/node_modules/.pnpm/astro@5.1.1_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules:/home/ben/Documents/project/website/cmhben/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.1.1_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.1.1_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/astro.js" "$@"
fi
