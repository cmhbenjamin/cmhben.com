#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benc/project/website/cmhben.com/node_modules/.pnpm/@skypack+package-check@0.2.2/node_modules/@skypack/package-check/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/@skypack+package-check@0.2.2/node_modules/@skypack/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/@skypack+package-check@0.2.2/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/benc/project/website/cmhben.com/node_modules/.pnpm/@skypack+package-check@0.2.2/node_modules/@skypack/package-check/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/@skypack+package-check@0.2.2/node_modules/@skypack/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/@skypack+package-check@0.2.2/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@skypack/package-check/index.bin.js" "$@"
else
  exec node  "$basedir/../@skypack/package-check/index.bin.js" "$@"
fi
