#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benc/project/website/cmhben.com/node_modules/.pnpm/astro@5.1.1_@types+node@18.19.68_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/astro@5.1.1_@types+node@18.19.68_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/benc/project/website/cmhben.com/node_modules/.pnpm/astro@5.1.1_@types+node@18.19.68_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/astro@5.1.1_@types+node@18.19.68_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules:/home/benc/project/website/cmhben.com/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.1.1_@types+node@18.19.68_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.1.1_@types+node@18.19.68_jiti@2.4.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/astro.js" "$@"
fi
